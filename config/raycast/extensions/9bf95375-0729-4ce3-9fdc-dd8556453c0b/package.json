{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vercast",
  "title": "Vercel",
  "description": "View and modify your Vercel projects, deployments and domains.",
  "icon": "icon.png",
  "license": "MIT",
  "author": "MaxLeiter",
  "owner": "vercel",
  "access": "public",
  "contributors": [
    "matt",
    "dgrcode",
    "samuelkraft",
    "xmok",
    "ridemountainpig",
    "sebdanielsson",
    "1weiho"
  ],
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Explore your Vercel projects",
      "mode": "view"
    },
    {
      "name": "search-deployments",
      "title": "Search Deployments",
      "description": "Explore your Vercel deployments",
      "mode": "view"
    },
    {
      "name": "menu-bar-deployments",
      "title": "Menu Bar Deployments",
      "description": "View recent deployments in the menu bar",
      "mode": "menu-bar",
      "interval": "15m",
      "preferences": [
        {
          "name": "maxDeployments",
          "title": "Max Deployments",
          "description": "The maximum number of deployments to show",
          "type": "textfield",
          "required": false,
          "placeholder": "10"
        }
      ]
    },
    {
      "name": "search-components",
      "title": "Search Components",
      "description": "Search the Vercel styleguide",
      "mode": "view"
    },
    {
      "name": "search-domains",
      "title": "Search Domains",
      "description": "Explore your Vercel domains",
      "mode": "view"
    },
    {
      "name": "check-domain-availability",
      "title": "Check Domain Availability",
      "description": "Check if a domain is available",
      "mode": "view",
      "arguments": [
        {
          "name": "domain",
          "description": "The domain to check",
          "type": "text",
          "placeholder": "example.com",
          "required": true
        }
      ]
    }
  ],
  "tools": [
    {
      "name": "get-teams",
      "title": "Get Teams",
      "description": "Get all the Vercel teams the user is a member of",
      "confirmation": false
    },
    {
      "name": "get-deployments",
      "title": "Get Deployments",
      "description": "Get all the Vercel deployments for a team",
      "input": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string"
          }
        },
        "required": []
      },
      "confirmation": false
    },
    {
      "name": "get-projects",
      "title": "Get Projects",
      "description": "Get all the Vercel projects for a team",
      "input": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string"
          }
        },
        "required": []
      },
      "confirmation": false
    },
    {
      "name": "delete-project",
      "title": "Delete Project",
      "description": "Delete a Vercel project",
      "input": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "required": [
          "projectId"
        ]
      },
      "confirmation": true
    },
    {
      "name": "get-domains",
      "title": "Get Domains",
      "description": "Get all the domains for a Vercel team",
      "input": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string"
          }
        },
        "required": [
          "teamId"
        ]
      },
      "confirmation": false
    },
    {
      "name": "get-environment-variables",
      "title": "Get Environment Variables",
      "description": "Get all the environment variables for a Vercel project",
      "input": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "required": [
          "projectId",
          "teamId"
        ]
      },
      "confirmation": false
    },
    {
      "name": "delete-environment-variable",
      "title": "Delete Environment Variable",
      "description": "Delete a Vercel environment variable",
      "input": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string"
          },
          "envId": {
            "type": "string"
          },
          "teamId": {
            "type": "string"
          }
        },
        "required": [
          "projectId",
          "envId",
          "teamId"
        ]
      },
      "confirmation": true
    },
    {
      "name": "add-environment-variable",
      "title": "Add Environment Variable",
      "description": "Add a Vercel environment variable",
      "input": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string"
          },
          "envVar": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "enum": [
                  "system",
                  "secret",
                  "encrypted",
                  "plain"
                ]
              },
              "target": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": [
                    "production",
                    "preview",
                    "development"
                  ]
                }
              }
            },
            "required": [
              "key",
              "value",
              "type",
              "target"
            ]
          },
          "teamId": {
            "type": "string"
          }
        },
        "required": [
          "projectId",
          "envVar"
        ]
      },
      "confirmation": false
    },
    {
      "name": "check-domain-availability",
      "title": "Check Domain Availability",
      "description": "Check if a domain is available",
      "input": {
        "type": "object",
        "properties": {
          "domain": {
            "type": "string"
          }
        },
        "required": [
          "domain"
        ]
      },
      "confirmation": false
    }
  ],
  "ai": {
    "evals": [
      {
        "input": "@vercast what's the preview url for my latest PR in raycast-frontend?",
        "mocks": {
          "get-teams": [
            {
              "name": "Raycast",
              "id": "team_123456789abcdefghijklmnf1"
            },
            {
              "name": "Other team",
              "id": "team_123456789abcdefghijklmnf1"
            }
          ],
          "get-projects": [
            {
              "name": "raycast-frontend",
              "id": "prj_a1UidQTQ12D4ejFoQMBD9I3byl2V"
            }
          ],
          "get-deployments": [
            {
              "url": "raycast-frontend.vercel.app",
              "name": "raycast-frontend",
              "state": "READY",
              "inspectorUrl": "https://vercel.com/raycast/raycast-frontend/123abc1",
              "created": 1737364657943
            },
            {
              "url": "raycast-frontend.vercel.app",
              "name": "raycast-frontend",
              "state": "READY",
              "inspectorUrl": "https://vercel.com/raycast/raycast-frontend/123abc3",
              "created": 1737364657945
            }
          ]
        },
        "expected": [
          {
            "callsTool": {
              "name": "get-teams"
            }
          },
          {
            "callsTool": {
              "name": "get-deployments"
            }
          }
        ]
      },
      {
        "input": "@vercast what's the status for my latest deployment in raycast-frontend?",
        "mocks": {
          "get-teams": [
            {
              "name": "Raycast",
              "id": "team_123456789abcdefghijklmnf1"
            },
            {
              "name": "Other team",
              "id": "team_123456789abcdefghijklmnf1"
            }
          ],
          "get-projects": [
            {
              "name": "raycast-frontend",
              "id": "prj_a1UidQTQ12D4ejFoQMBD9I3byl2V"
            }
          ],
          "get-deployments": [
            {
              "url": "raycast-frontend.vercel.app",
              "name": "raycast-frontend",
              "state": "READY",
              "inspectorUrl": "https://vercel.com/raycast/raycast-frontend/123abc1",
              "created": 1737364657943
            },
            {
              "url": "raycast-frontend.vercel.app",
              "name": "raycast-frontend",
              "state": "READY",
              "inspectorUrl": "https://vercel.com/raycast/raycast-frontend/123abc3",
              "created": 1737364657945
            }
          ]
        },
        "expected": [
          {
            "callsTool": {
              "name": "get-teams"
            }
          },
          {
            "callsTool": {
              "name": "get-deployments",
              "arguments": {
                "teamId": "team_123456789abcdefghijklmnf1"
              }
            }
          }
        ]
      },
      {
        "input": "@vercast get the domains for team raycast",
        "mocks": {
          "get-teams": [
            {
              "name": "Raycast",
              "id": "team_123456789abcdefghijklmnf1"
            }
          ],
          "get-domains": [
            {
              "name": "raycast.com"
            }
          ]
        },
        "expected": [
          {
            "callsTool": {
              "name": "get-teams"
            }
          },
          {
            "callsTool": {
              "name": "get-domains",
              "arguments": {
                "teamId": "team_123456789abcdefghijklmnf1"
              }
            }
          }
        ]
      },
      {
        "input": "@vercast add \"123abc\" as KEY env variable to raycast-frontend",
        "mocks": {
          "get-teams": [
            {
              "name": "Raycast",
              "id": "team_vQ7Eag8KdhOGpsdvxtCGN1ff"
            }
          ],
          "get-projects": [
            {
              "name": "raycast-frontend",
              "id": "prj_B9ZgxXPDnZFRIsf3Xh6I3oKRvdGz"
            }
          ],
          "add-environment-variable": "Successfully called \"add-environment-variable\""
        },
        "expected": [
          {
            "callsTool": {
              "arguments": {},
              "name": "get-teams"
            }
          },
          {
            "callsTool": {
              "arguments": {
                "teamId": "team_vQ7Eag8KdhOGpsdvxtCGN1ff"
              },
              "name": "get-projects"
            }
          },
          {
            "callsTool": {
              "name": "add-environment-variable"
            }
          }
        ]
      },
      {
        "input": "@vercast delete the KEY env variable in raycast-frontend",
        "mocks": {
          "get-teams": [
            {
              "name": "Raycast",
              "id": "team_vQ7Eag8KdhOGpsdvxtCGN1ff"
            }
          ],
          "get-projects": [
            {
              "name": "raycast-frontend",
              "id": "prj_B9ZgxXPDnZFRIsf3Xh6I3oKRvdGz"
            }
          ],
          "get-environment-variables": [
            {
              "key": "KEY",
              "value": "123abc"
            }
          ],
          "get-deployments": [
            {
              "url": "raycast-frontend.vercel.app",
              "name": "raycast-frontend",
              "state": "READY",
              "inspectorUrl": "https://vercel.com/raycast/raycast-frontend/123abc1",
              "created": 1737364657943
            }
          ],
          "delete-environment-variable": "Successfully called \"delete-environment-variable\""
        },
        "expected": [
          {
            "callsTool": {
              "name": "get-teams",
              "arguments": {}
            }
          },
          {
            "callsTool": {
              "name": "get-projects",
              "arguments": {
                "teamId": "team_vQ7Eag8KdhOGpsdvxtCGN1ff"
              }
            }
          },
          {
            "callsTool": {
              "name": "delete-environment-variable",
              "arguments": {
                "envId": "KEY",
                "teamId": "team_vQ7Eag8KdhOGpsdvxtCGN1ff"
              }
            }
          }
        ]
      },
      {
        "input": "@vercast Check if vercast.com domain is available",
        "mocks": {
          "check-domain-availability": [
            {
              "domain": "raycast.com"
            }
          ]
        },
        "expected": [
          {
            "callsTool": {
              "arguments": {
                "domain": "raycast.com"
              },
              "name": "check-domain-availability"
            }
          }
        ]
      }
    ]
  },
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Vercel access token",
      "placeholder": "Enter your Access Token",
      "link": "https://vercel.com/account/tokens"
    }
  ],
  "dependencies": {
    "@raycast/api": "~1.102.3",
    "@raycast/utils": "^2.2.0",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "eslint": "^9.32.0",
    "react": "^19.1.1",
    "typescript": "~5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}